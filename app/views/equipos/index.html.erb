<p id="notice"><%= notice %></p>

<h1>Equipos</h1>

<table>
  <thead>
    <tr>
      <th>Idequipo</th>
      <th>Placa</th>
      <th>Modelo</th>
      <th>Serie</th>
      <th>Marca</th>
      <th>Idestadoequipo</th>
      <th>Idtipoequipo</th>
      <th>Idarea</th>
      <th>Eliminado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @equipos.each do |equipo| %>
      <tr>
        <td><%= equipo.IdEquipo %></td>
        <td><%= equipo.Placa %></td>
        <td><%= equipo.Modelo %></td>
        <td><%= equipo.Serie %></td>
        <td><%= equipo.Marca %></td>
        <td><%= equipo.IdEstadoEquipo %></td>
        <td><%= equipo.IdTipoEquipo %></td>
        <td><%= equipo.IdArea %></td>
        <td><%= equipo.eliminado %></td>
        <td><%= link_to 'Show', equipo %></td>
        <td><%= link_to 'Edit', edit_equipo_path(equipo) %></td>
        <td><%= link_to 'Destroy', equipo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Equipo', new_equipo_path %>
